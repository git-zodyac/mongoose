{
  "name": "@zodyac/zod-mongoose",
  "version": "4.2.1",
  "description": "A library that allows you to generate mongoose schemas from zod objects.",
  "scripts": {
    "test": "jest",
    "test:run": "node test.js",
    "test:badges": "jest --coverage && make-coverage-badge --report-path ./artifacts/coverage/coverage-summary.json --output-path ./badges/coverage.svg",
    "build": "tsup",
    "lint": "biome check",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "private": false,
  "sideEffects": true,
  "files": [
    "dist"
  ],
  "author": "bebrasmell",
  "license": "MIT",
  "homepage": "https://zodyac.dev/toolbox/zod-mongoose",
  "repository": {
    "type": "git",
    "url": "https://github.com/git-zodyac/mongoose"
  },
  "keywords": [
    "zod",
    "mongoose",
    "schema",
    "validation",
    "typescript",
    "type",
    "types",
    "type-safe",
    "validation",
    "mongodb",
    "mongo",
    "database",
    "db",
    "orm",
    "odm",
    "document"
  ],
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@swc/core": "1.15.3",
    "@swc/jest": "0.2.39",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.1",
    "jest": "30.2.0",
    "make-coverage-badge": "1.2.0",
    "mongoose": "8.20.2",
    "tsup": "8.5.1",
    "typescript": "5.9.2",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "mongoose": "^8.20.2",
    "zod": "^3.22.4"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
